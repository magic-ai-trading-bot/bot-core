[run]
omit =
    */tests/*
    */test_*.py
    example_client.py
    test_setup.py
    start.py
    utils/redis_cache.py
    tests/test_ml_compatibility.py
    tests/test_ml_performance.py
    */site-packages/*
    */__pycache__/*
    celery_app.py
    tasks/*
    app/core/*
    app/websocket/*
    app/models/*
    generate_openapi.py

[report]
# Note: fail_under removed - threshold check happens in CI workflow after all test steps complete
# Individual test steps (step 1, 2, 3) may have <93% coverage, only final merged coverage matters
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
